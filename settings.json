{
  "workbench.startupEditor": "newUntitledFile",
  "editor.wordWrap": "off",
  "git.path": [
    "/usr/bin/git"
  ],
  "editor.formatOnSave": true,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter"
  },
  "editor.tabSize": 2,
  "workbench.editor.enablePreview": false,
  "editor.fontFamily": "\"Fira Code\", Menlo, Monaco, 'Courier New', monospace",
  "editor.wordSeparators": "`~!@#$%^&*()-=+[{]}\\|;:'\",.<>/?_",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.options": {},
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.addMissingImports": "explicit"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.rulers": [
    150
  ],
  "go.toolsManagement.autoUpdate": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "tailwindCSS.includeLanguages": {
    "plaintext": "javascript"
  },
  "search.followSymlinks": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "explorer.confirmDelete": false,
  "liveServer.settings.donotVerifyTags": true,
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#1073cf2d",
    "editor.lineHighlightBorder": "#9fced11f",
  },
  "editor.guides.bracketPairs": false,
  "editor.inlineSuggest.enabled": true,
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "git.ignoreMissingGitWarning": true,
  "diffEditor.wordWrap": "off",
  "editor.guides.indentation": false,
  "editor.quickSuggestionsDelay": 0,
  "editor.inlayHints.enabled": false,
  "workbench.sideBar.location": "right",
  "extensions.ignoreRecommendations": true,
  "tailwindCSS.experimental.classRegex": [
    [
      "cva\\(([^)]*)\\)",
      "[\"'`]([^\"'`]*).*?[\"'`]"
    ]
  ],
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": true,
    "scminput": false,
    "yaml": false,
    "typescript": true,
    "javascript": true
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "indenticator.color.light": "#cccccc",
  "indenticator.color.dark": "#cccccc",
  "indenticator.inner.color.light": "#dddddd",
  "workbench.editor.empty.hint": "hidden",
  "rapidapi.terminalLink.enabled": false,
  "workbench.colorTheme": "NetBeans Light Theme",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.lineHeight": 1.2,
  "terminal.integrated.letterSpacing": 1,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "git.openRepositoryInParentFolders": "never",
  "gitlens.ai.model": "anthropic:claude-3-5-sonnet-latest"
}